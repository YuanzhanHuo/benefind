                                        <button class="btn-primary quiz-nav-btn" id="core-btn-next"> Next</button>
                                    </div>
                                </div>

                            </div>
                            <!-- DailyLiving quize-->
                            <div class="tab-pane fade" id="v-pills-DailyLiving" role="tabpanel" aria-labelledby="v-pills-DailyLiving-tab">
                                <div class="quize-container d-flex" role="tabpanel" aria-labelledby="v-pills-DailyLiving-tab">

                                    <div class="Single-Quize-Container">
                                        <p class="circleGroup">
                                            <i class="numberCircle">1</i>
